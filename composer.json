{
    "name": "spatie-weight-conversions-course/spatie-weight-conversions-course",
    "description": "This is my package spatie-weight-conversions-course",
    "keywords": [
        "spatie-weight-conversions-course",
        "spatie-weight-conversions-course"
    ],
    "homepage": "https://github.com/spatie-weight-conversions-course/spatie-weight-conversions-course",
    "license": "MIT",
    "authors": [
        {
            "name": "iamopk",
            "email": "no@mail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "iamopk\\SpatieWeightConversionsCourse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iamopk\\SpatieWeightConversionsCourse\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
